pipeline {
    agent any
         stages {
         
          stage('Build') {
            steps {
                echo 'Building'
            }
          }
            stage('Deploy') {
            steps {
                echo 'deploying'
            }
            }
             stage('Test') {
            steps {
                echo 'Testing'
            }
             }
             stage('Hello') {
            steps {
                echo 'Hello World'
            }
             }
              stage('Release') {
            steps {
                echo 'releasing'
            }
              }
         }
    
}
